model = 'kouteimaster'

$('form#new_' + model).clear_previous_errors()

<% if @kouteimaster.errors.any? %>
  $('form#new_' + model).render_form_errors(model, '<%=@kouteimaster.errors.messages.to_json.html_safe%>')
<% else %>
  $('#koutei-new-modal').modal('hide');
  $('#kouteimaster').DataTable().row.add( [
    "<%=@kouteimaster.id%>",
    "<%=@kouteimaster.所属コード%>",
    "<%=@kouteimaster.shozokumaster_name%>",
    "<%=@kouteimaster.code%>",
    "<%=@kouteimaster.name%>"
  ]).draw( false );
<% end %>
