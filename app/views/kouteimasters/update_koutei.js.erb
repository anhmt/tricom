model = 'kouteimaster'

$('form#edit_' + model).clear_previous_errors()

<% if @kouteimaster.errors.any? %>
  $('form#edit_' + model).render_form_errors(model, '<%=@kouteimaster.errors.messages.to_json.html_safe%>')
<% else %>
  $('#koutei-edit-modal').modal('hide');
  $("#kouteimaster").DataTable().row('tr.selected').remove();
  $('#kouteimaster').DataTable().row.add([
    "<%=@kouteimaster.id%>",
    "<%=@kouteimaster.所属コード%>",
    "<%=@kouteimaster.shozokumaster_name%>",
    "<%=@kouteimaster.code%>",
    "<%=@kouteimaster.name%>"
    ]).draw( false );
  $('#kouteimaster').DataTable().buttons('#edit, #destroy').nodes().addClass('disabled');
<% end %>