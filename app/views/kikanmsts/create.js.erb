model = 'kikanmst'

$('form#new_' + model).clear_previous_errors()

<% if @kikanmst.errors.any? %>
  $('form#new_' + model).render_form_errors(model, '<%=@kikanmst.errors.messages.to_json.html_safe%>')
<% else %>
  $('#kikan-new-modal').modal('hide');
  $('#kikanmaster').DataTable().row.add( [
    "<%=@kikanmst.機関コード%>",
    "<%=@kikanmst.機関名%>",
    "<%=@kikanmst.備考%>"
  ]).draw( false );
<% end %>
