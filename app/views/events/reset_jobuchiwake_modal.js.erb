var table = $('#jobuchiwake_table').DataTable();
table.clear().draw();
<% if @jobuchiwakes.any? %>
  <% @jobuchiwakes.each do |jobuchiwake| %>
    table.row.add( [
      "<%= jobuchiwake.ジョブ内訳番号%>",
      "<%= jobuchiwake.受付日時&.strftime('%Y/%m/%d %H:%M') %>",
      "<%= jobuchiwake.件名 %>",
      "<%= jobuchiwake.受付種別 %>"
      ] ).draw(false);
  <% end %>
  $('.row#uchiwake').show();
<% else %>
  $('.row#uchiwake').hide();
<% end %>