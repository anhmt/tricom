.row-fruid
  .form-inline
    .form-group
      .col-lg-11
        = form_for :setsubiyoyaku, url: setsubiyoyakus_path, html: {class:'form-inline'}, method: :get do |f|
          = hidden_field_tag 'selected_date', @selected_date
          = label_tag t('title.setsubi1')
          = select(:head, :setsubicode, Setsubi.all.order(:設備コード).pluck(:設備名, :設備コード), {selected: @setsubi_param, include_blank: true }, class: 'form-control')
      .col-lg-1
        = link_to '設備予約週表示', timeline7Day_setsubiyoyakus_path, class: 'btn btn-primary'
