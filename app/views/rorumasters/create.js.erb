model = 'rorumaster'

$('form#new_' + model).clear_previous_errors()

<% if @rorumaster.errors.any? %>
  $('form#new_' + model).render_form_errors(model, '<%=@rorumaster.errors.messages.to_json.html_safe%>')
<% else %>
  $('#roru-new-modal').modal('hide');
  $('#rorumaster').DataTable().row.add( [
    "<%=@rorumaster.ロールコード%>",
    "<%=@rorumaster.ロール名%>",
    "<%=@rorumaster.序列%>"
  ]).draw( false );
<% end %>
