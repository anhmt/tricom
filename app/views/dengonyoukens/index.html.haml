= javascript_include_tag 'dengonyoukens/index'
= provide(:title, (t 'title.dengonyouken'))
%ol.breadcrumb
  %li= link_to (t 'title.menu'), main_path
  %li.active= (t 'title.dengonyouken')

%table#dengonyouken.table.table-striped.table-bordered.table-hover{cellspacing: 0, width: '100%' }
  %thead
    %tr
      %th ID
      %th= (t 'title.shuruimei')
      %th= (t 'title.bikou')
      %th= (t 'title.yuusensa')
      %th
      %th
  %tbody
    = render @dengonyoukens
= render 'new'
= render 'edit'
