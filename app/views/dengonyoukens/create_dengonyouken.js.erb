model = 'dengonyouken'

$('form#new_' + model).clear_previous_errors()

<% if @dengonyouken.errors.any? %>
  $('form#new_' + model).render_form_errors(model, '<%=@dengonyouken.errors.messages.to_json.html_safe%>')
<% else %>
  $('#dengonyouken-new-modal').modal('hide');
  $('#dengonyoukenmaster').DataTable().row.add( [
    "<%=@dengonyouken.id%>",
    "<%=@dengonyouken.種類名%>",
    "<%=@dengonyouken.備考%>",
    "<%=@dengonyouken.優先さ%>"
  ]).draw( false );
<% end %>
