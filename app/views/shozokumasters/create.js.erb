model = 'shozokumaster'

$('form#new_' + model).clear_previous_errors()

<% if @shozokumaster.errors.any? %>
  $('form#new_' + model).render_form_errors(model, '<%=@shozokumaster.errors.messages.to_json.html_safe%>')
<% else %>
  $('#shozoku-new-modal').modal('hide');
  $('#shozokumaster').DataTable().row.add( [
    "<%=@shozokumaster.所属コード%>",
    "<%=@shozokumaster.所属名%>"
  ]).draw( false );
<% end %>
