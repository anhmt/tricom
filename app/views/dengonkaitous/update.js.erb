model = 'dengonkaitou'

$('form#edit_' + model).clear_previous_errors()

<% if @dengonkaitou.errors.any? %>
  $('form#edit_' + model).render_form_errors(model, '<%=@dengonkaitou.errors.messages.to_json.html_safe%>')
<% else %>
  $('#dengonkaitou-edit-modal').modal('hide');
  $("#dengonkaitoumaster").DataTable().row('tr.selected').remove();
  $('#dengonkaitoumaster').DataTable().row.add([
    "<%=@dengonkaitou.id%>",
    "<%=@dengonkaitou.種類名%>",
    "<%=@dengonkaitou.備考%>"
    ]).draw( false );
  $('#dengonkaitoumaster').DataTable().buttons('#edit, #destroy').nodes().addClass('disabled');
<% end %>
