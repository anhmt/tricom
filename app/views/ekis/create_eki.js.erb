model = 'eki'

$('form#new_' + model).clear_previous_errors()

<% if @eki.errors.any? %>
  $('form#new_' + model).render_form_errors(model, '<%=@eki.errors.messages.to_json.html_safe%>')
<% else %>
  $('#eki-new-modal').modal('hide');
  $('#ekimaster').DataTable().row.add( [
    "<%=@eki.id%>",
    "<%=@eki.name%>",
    "<%=@eki.駅名カナ%>"
  ]).draw( false );
<% end %>
